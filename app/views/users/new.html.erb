

  <h1>Signup!</h1>
  <br>
  <%= form_tag '/users', method: :post do  %>
    <div><br>First Name: <br><input type="text" name="first_name"></div>
  
     <div><br>Last Name:<br><input type="text" name="last_name"></div>
  
    <div><br>Email:<br><input type="email" name="email"></div>
  
    <div><br>Phone Number:<br><input type="text" name="phone_number"></div>
  
    <div><br>Password:<br><input type="password" name="password"></div>
  
    <div><br>Confirm Password:<br><input type="password" name="password_confirmation"></div>
  
    <input type="submit" value="Create Account">
  <% end %>  
